# Adresse du site 
wrapvintage.duckdns.org {
    encode gzip
    log

    # Redirige toutes les requêtes /api/* vers le backend
    handle_path /api/* {
        reverse_proxy backend:${PORT}
    }

    # Tout le reste → frontend
    handle_path / {
        reverse_proxy frontend:${PORT_FRONT}
    }

}
